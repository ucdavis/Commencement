<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="Commencement.Core" namespace="Commencement.Core.Domain">
  <class name="TermCode" table="TermCodes">
    <id name="Id" column="id" unsaved-value="0">
      <generator class="assigned" />
    </id>

    <property name="Name" column="Name" />
    <property name="IsActive" column="IsActive" />
   
    <bag name="Ceremonies" cascade="none">
      <key column="TermCode"></key>
      <one-to-many class="Ceremony"/>
    </bag>
    
  </class>
</hibernate-mapping>