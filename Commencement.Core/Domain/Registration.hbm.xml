<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="Commencement.Core" namespace="Commencement.Core.Domain">
  <class name="Registration" table="Registrations" >
    <id name="Id" column="id">
      <generator class="identity" />
    </id>

    <many-to-one name="Student" class="Student" column="Student_Id" fetch="join"/>
    <many-to-one name="Major" class="MajorCode" column="MajorCode" />
    <property name="Address1" column="Address1" />
    <property name="Address2" column="Address2" />
    <property name="Address3" column="Address3" />
    <property name="City" column="City" />
    <many-to-one name="State" class="State" column="State" />
    <property name="Zip" column="Zip" />
    <property name="Email" column="Email" />
    <property name="NumberTickets" column="NumberTickets" />
    <property name="MailTickets" column="MailTickets" />
    <property name="Comments" column="Comments" />
    <many-to-one name="Ceremony" class="Ceremony" column="CeremonyId" />
    <many-to-one name="ExtraTicketPetition" class="ExtraTicketPetition" column="ExtraTicketPetitionId" cascade="all-delete-orphan" />
    <property name="DateRegistered" column="DateRegistered" />
    
  </class>
</hibernate-mapping>