<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" 
                   assembly="Commencement.Core" namespace="Commencement.Core.Domain">
  <class
    name="CeremonyEditor"
    table="CeremonyEditors">
    <id name="Id" column="ID">
      <generator class="identity" />
    </id>

    <property name="LoginId" column="LoginId" />
    <property name="Owner" column="Owner" />

    <many-to-one class="Ceremony" name="Ceremony" column="CeremonyId"></many-to-one>
    
  </class>
</hibernate-mapping>