<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="Commencement.Core" namespace="Commencement.Core.Domain">
  <class
    name="ExtraTicketPetition"
    table="ExtraTicketPetitions">
    <id name="Id" column="ID">
      <generator class="identity" />
    </id>

    <property name="NumberTickets" column="NumberTickets" />
    <property name="IsPending" column="IsPending" />
    <property name="IsApproved" column="IsApproved" />

    <property name="DateSubmitted" column="DateSubmitted" />
    <property name="DateDecision" column="DateDecision" />
    
  </class>
</hibernate-mapping>