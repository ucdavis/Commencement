  <DataSets>
    <DataSet Name="DataSet1">
      <Fields>
        <Field Name="StudentId">
          <DataField>StudentId</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="LastName">
          <DataField>LastName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="FirstName">
          <DataField>FirstName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="email">
          <DataField>email</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="RegistrationTickets">
          <DataField>RegistrationTickets</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="CeremonyTime">
          <DataField>CeremonyTime</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>Commencement</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>select students.StudentId, students.LastName, students.FirstName
	, isnull(reg.email, students.email) email
	, rp.NumberTickets RegistrationTickets
	, c.datetime CeremonyTime
from RegistrationParticipations rp
	inner join Registrations reg on rp.RegistrationId = reg.id
	inner join Students on students.Id = reg.Student_Id
	left outer join ExtraTicketPetitions etp on etp.id = rp.ExtraTicketPetitionId and etp.IsPending = 1 and etp.IsApproved = 1
	inner join ceremonies c on rp.CeremonyId = c.id
where students.SJABlock = 0
	and rp.Cancelled = 0
	and rp.CeremonyId = @cid
order by lastname</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <QueryParameters>
          <QueryParameter Name="@cid">
            <Value>=Parameters!cid.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <Timeout>0</Timeout>
      </Query>
    </DataSet>
  </DataSets>
  <ReportParameters>
    <ReportParameter Name="cid">
      <DataType>String</DataType>
      <Prompt>cid</Prompt>
    </ReportParameter>
